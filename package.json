{
  "name": "database",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "docker-compose up -d",
    "stop": "docker-compose down",
    "pg:restore": "docker exec -it postgres /bin/bash --login /root/.load_backups",
    "pg:client": "docker exec -it postgres psql -U postgres -W",
    "idb:restore": "docker exec -it influxdb /bin/bash --login /root/.load_backups",
    "idb:client": "docker exec -it influxdb /bin/bash --login",
    "n4j:restore": "docker exec -it neo4j /bin/bash --login /root/.load_backups",
    "n4j:client": "docker exec -it neo4j cypher-shell -u neo4j -p secret",
    "rdb:restore": "docker exec -it redis /bin/bash --login /root/.load_backups",
    "rdb:client": "docker exec -it redis redis-cli",
    "preinstall": "npx only-allow yarn"
  }
}
